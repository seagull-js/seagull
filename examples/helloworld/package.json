    "@seagull/deploy-aws": "^16.0.0",
    "@seagull/test-e2e": "^16.0.0",
    "@seagull/test-pages": "^16.0.0",
    "@seagull/tslint": "^16.0.0",
    "@types/lodash": "^4.14.123",
    "@types/node": "^8.10",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "serverless": "^1.32.0",
    "serverless-apigw-binary": "^0.4.4",
