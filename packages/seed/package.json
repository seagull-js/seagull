{
  "name": "@seagull/seed",
  "version": "12.0.0",
  "description": "Seed implementation for the seagull framework",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc && npm run lint",
    "test": "tslint --fix src/**/*.ts test/**/*.ts && nyc mocha --opts test/mocha.opts",
    "test:watch": "npm run test -- --watch --reporter min",
    "lint": "tslint --project ./tsconfig.json --config ./tslint.json --fix './{src, test}/**/*.ts'",
    "docs": "rm -rf docs && typedoc --mode modules --ignoreCompilerErrors --excludeExternals --excludePrivate --excludeNotExported --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seagull-js/seagull.git"
  },
  "keywords": [
    "web",
    "framework",
    "web framework",
    "serverless",
    "cloud",
    "aws",
    "react",
    "react.js",
    "isomorphic"
  ],
  "author": "Christian PÃ¤tzold",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/seagull-js/seagull/issues"
  },
  "homepage": "https://github.com/seagull-js/seagull#readme",
  "dependencies": {
    "@seagull/libraries": "^12.0.0"
  },
  "devDependencies": {
    "@seagull/testing": "^12.0.0",
    "@seagull/tslint": "^12.0.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      "dist",
      "docs",
      "node_modules",
      "test"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3a0b7dd08da1d19ac6036eaebeb079386be12629"
}
